; Test a power event session.
;
; Variant: tracing is disabled at the same IP as the power session.
;
; opt:ptxed --event:ip
;

org 0x1000
bits 64

; @pt p0: psb()
; @pt p1: mode.exec(64bit)
; @pt p2: fup(3: %l0)
; @pt p3: psbend()
l0: nop

; @pt p4: exstop(ip)
; @pt p5:fup(1: %l1)
l1: nop

; @pt p6:fup(1: %l1)
; @pt p7:tip.pgd(0: %l2)
l2: hlt


