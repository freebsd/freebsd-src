; Test MTC-based TSC estimation.
;
; Variant: omit some MTC
;
; opt:ptdump --time --time-delta --no-tcal
; opt:ptdump --mtc-freq 0 --cpuid-0x15.eax 2 --cpuid-0x15.ebx 8

org 0x100000
bits 64

; @pt p1: psb()
; @pt p2: psbend()

; @pt p3: tsc(0xa0000)
; @pt p4: tma(0x1, 0x4)
; @pt p5: mtc(0x4)
; @pt p6: mtc(0xa)


; @pt .exp(ptdump)
;%0p1   psb
;%0p2   psbend
;%0p3   tsc a0000       tsc +a0000
;%0p4   tma 1, 4        tsc +0
;%0p5   mtc 4           tsc +8
;%0p6   mtc a           tsc +18
