; Test packet generation overflow
;
; Variant: tracing disabled, timing packets in-between
;

org 0x1000
bits 64

; @pt p0:  psb()
; @pt p1:  mode.exec(32bit)
; @pt p2:  fup(3: %l0)
; @pt p3:  psbend()
l0: hlt

; @pt p4:  ovf()
; @pt p5:  tsc(0xa00)
; @pt p6:  cbr(0xf)
; @pt p7:  tma(0xa, 0xc)
; @pt p8:  pad()
; @pt p9:  mtc(0x10)
; @pt p10: cyc(0xa)
; @pt p11: mode.exec(64bit)
; @pt p12: tip.pge(3: %l1)
l1: nop

; @pt p13: fup(1: %l2)
; @pt p14: tip.pgd(0: %l3)
l2: nop
l3: hlt


