; Test packet generation overflow
;
; Variant: tracing remains enabled, mnt in-between
;

org 0x1000
bits 64

; @pt p0: psb()
; @pt p1: mode.exec(64bit)
; @pt p2: fup(3: %l0)
; @pt p3: psbend()
l0: hlt

; @pt p4: ovf()
; @pt p5: pad()
; @pt p6: mnt(0xa000)
; @pt p7: fup(3: %l1)
l1: nop

; @pt p8: fup(1: %l2)
; @pt p9: tip.pgd(0: %l3)
l2: nop
l3: hlt


