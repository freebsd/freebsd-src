; Test MTC and CYC-based TSC estimation.
;
; Variant: CBR-based calibration, correct using MTC-based calibration
;
; opt:ptdump --time --time-delta
; opt:ptdump --nom-freq 4 --mtc-freq 4 --cpuid-0x15.eax 1 --cpuid-0x15.ebx 4

org 0x100000
bits 64

; @pt p1: psb()
; @pt p2: psbend()

; @pt p3:  tsc(0xa0000)
; @pt p4:  tma(0x18, 0x8)
; @pt p5:  cbr(0x2)
; @pt p6:  mtc(0x2)
; @pt p7:  cyc(0x100)
; @pt p8:  mtc(0x3)
; @pt p9:  cyc(0x100)
; @pt p10: mtc(0x4)


; @pt .exp(ptdump)
;%0p1    psb
;%0p2    psbend
;%0p3    tsc a0000       tsc +a0000
;%0p4    tma 18, 8       tsc +0
;%0p5    cbr 2
;%0p6    mtc 2           tsc +18
;%0p7    cyc 100         tsc +200
;%0p8    mtc 3           tsc -1c0
;%0p9    cyc 100         tsc +40
;%0p10   mtc 4           tsc +0
