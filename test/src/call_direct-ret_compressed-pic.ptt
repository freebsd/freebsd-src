; Test a compressed return for a direct call
;
; Variant: ignore a call with zero displacement
;

org 0x100000
bits 32

; @pt p1: psb()
; @pt p2: fup(3: %l1)
; @pt p3: mode.exec(32bit)
; @pt p4: psbend()
l1:     call l4
l2:     jz l6
l3:     hlt

; @pt p5: tnt(t.t)
l4:     call l5
l5:     ret

; @pt p6: fup(1: %l6)
; @pt p7: tip.pgd(0: %l7)
l6:     nop
l7:     hlt


