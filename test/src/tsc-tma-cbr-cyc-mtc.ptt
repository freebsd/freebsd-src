; Test MTC and CYC-based TSC estimation.
;
; Variant: CBR-based calibration,
;          CYC between TMA and MTC, time corrected on MTC
;
; opt:ptdump --time --time-delta
; opt:ptdump --nom-freq 4 --mtc-freq 4 --cpuid-0x15.eax 2 --cpuid-0x15.ebx 1

org 0x100000
bits 64

; @pt p1: psb()
; @pt p2: psbend()

; @pt p3: tsc(0xa0000)
; @pt p4: tma(0x12, 0x4)
; @pt p5: cbr(0x2)
; @pt p6: cyc(0x3)
; @pt p7: cyc(0x1)
; @pt p8: mtc(0x2)


; @pt .exp(ptdump)
;%0p1   psb
;%0p2   psbend
;%0p3   tsc a0000       tsc +a0000
;%0p4   tma 12, 4       tsc +0
;%0p5   cbr 2
;%0p6   cyc 3           tsc +6
;%0p7   cyc 1           tsc +2
;%0p8   mtc 2           tsc -5
