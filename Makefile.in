unbound-control-setup:	$(srcdir)/smallapp/unbound-control-setup.sh
	sed -e 's:^DESTDIR=.*$$:DESTDIR=$(UNBOUND_RUN_DIR):' < $(srcdir)/smallapp/unbound-control-setup.sh > $@
	-chmod +x $@

