     (+ pack GRk OP_0B GRi (ICCi_1-null) OPE2_00 GRj)
     (scan-semantics GRi GRj GRk)
     ((fr400 (unit u-integer))
      (fr500 (unit u-integer)) (fr550 (unit u-integer)))
