(define-pmacro (jmprel name insn opc1)
  (dni name
       (.str name "conditional" )
       (COND-CTI (PIPE OS) (IDOC JMP))
       (.str insn " $cond,$rel")
       (+ opc1 OP2_13 rel)
       (sequence ()
           (if (eq cond (const 1))
               (sequence ()
		    (if QI (lt QI rel (const 0))
                           (sequence ()
                                (neg QI rel)
                                (add QI rel (const 1))
                                (mul QI rel (const 2))
                                (set HI pc (sub HI pc rel))
                           ))
                    (set HI pc (add HI pc (mul QI rel (const 2))))
               )
            )
            (set HI pc pc)
       )
       ()
  )
)

