(df f-dsp-40-s16 "16 bit   signed" (all-isas) 40 16 INT
     ((value pc) (ext INT 
		      (trunc HI
			     (or (and (srl value 8) #x00ff)
				 (and (sll value 8) #xff00))))) ; insert
     ((value pc) (ext INT 
