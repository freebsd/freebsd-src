		     (and (srl value 8) #x00ff)
		     (and (sll value 8) #xff00))) ; insert
     ((value pc) (or UHI
		     (and UHI (srl UHI value 8) #x00ff)
		     (and UHI (sll UHI value 8) #xff00))) ; extract
)

(df f-dsp-32-s16 "16 bit   signed" (all-isas) 32 16 INT
