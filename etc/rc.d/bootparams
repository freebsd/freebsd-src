#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/bootparams,v 1.7.10.1.8.1 2010/12/21 17:10:29 kensmith Exp $
#

# PROVIDE: bootparams
# REQUIRE: rpcbind DAEMON
# BEFORE:  LOGIN
# KEYWORD: nojail

. /etc/rc.subr

name="bootparamd"
rcvar=`set_rcvar`
required_files="/etc/bootparams"
command="/usr/sbin/${name}"

load_rc_config $name
run_rc_command "$1"
