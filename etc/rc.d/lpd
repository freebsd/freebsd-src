#!/bin/sh
#
# $NetBSD: lpd,v 1.5 2002/03/22 04:33:59 thorpej Exp $
# $FreeBSD: src/etc/rc.d/lpd,v 1.5.2.2 2006/01/11 06:06:53 brooks Exp $
#

# PROVIDE: lpd
# REQUIRE: DAEMON
# BEFORE:  LOGIN

. /etc/rc.subr

name="lpd"
rcvar=`set_rcvar`
command="/usr/sbin/${name}"
required_files="/etc/printcap"
start_precmd="_chkprintcap"

_chkprintcap()
{
	if checkyesno chkprintcap_enable ; then
		/usr/sbin/chkprintcap ${chkprintcap_flags}
	fi
}

load_rc_config $name
run_rc_command "$1"
