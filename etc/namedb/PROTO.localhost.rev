; $FreeBSD$
;
; This file is automatically edited by the `make-localhost' script in
; the /etc/namedb directory.
;

$TTL	3600

@	IN	SOA	@host@. root.@host@.  (
				@date@	; Serial
				3600	; Refresh
				900	; Retry
				3600000	; Expire
				3600 )	; Minimum
	IN	NS	@host@.
1	IN	PTR	localhost.@domain@.
