deg = 3;

a = 0.5;
b = 1;


f = x^(1/3);

poly = fpminimax(f, deg, [|single ...|], [a;b]);

display = hexadecimal;
print("rel error:", accurateinfnorm(1-poly(x)/f(x), [a;b], 30));
print("in [",a,b,"]");
print("coeffs:");
for i from 0 to deg do round(coeff(poly,i), SG, RN);
