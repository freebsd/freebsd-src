; config options
server:
	target-fetch-policy: "0 0 0 0 0"
	log-time-ascii: yes
	val-override-date: '20091018111500'
	fake-sha1: yes
stub-zone:
	name: "."
	stub-addr: 193.0.14.129         # K.ROOT-SERVERS.NET.
AUTOTRUST_FILE example.com
; autotrust trust anchor file
