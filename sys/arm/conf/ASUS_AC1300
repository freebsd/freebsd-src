#NO_UNIVERSE

include		"std.armv7"
include		"std.qca"

cpu		CPU_CORTEXA
ident		ASUS_AC1300
machine		arm armv7

# Use a low VA here so we get the early printf stuff working all the
# way up to cninit().
#options SOCDEV_PA=0x07800000
#options SOCDEV_VA=0x07800000
#options EARLY_PRINTF
options 	BOOTVERBOSE

options 	FDT_DTB_STATIC
makeoptions 	FDT_DTS_FILE=qcom-ipq4018-rt-ac58u.dts

options 	LINUX_BOOT_ABI
options 	SCHED_ULE
# DEFINITELY not ready for SMP yet!
# options 	SMP
options 	PLATFORM

device 		loop
device 		pty
device 		md
device 		gpio

device 		ether
device 		mii
device 		bpf
