			  "CCI_I2C_SDA2",
			  "CCI_I2C_SCL2",
			  "CCI_I2C_SDA3",
			  "CCI_I2C_SCL3",
			  "NC",
			  "DEBUG_GPIO1",
			  "RGBC_IR_INT",
			  "USB_CC_DIR",
			  "NC",
