			  "CAM_SOF",
			  "CAM2_RST_N",
			  "CAM_MCLK0",
			  "CAM_MCLK1",
			  "CAM_MCLK2",
			  "CAM_MCLK3",
			  "CCI_I2C_SDA0",
			  "CCI_I2C_SCL0",
