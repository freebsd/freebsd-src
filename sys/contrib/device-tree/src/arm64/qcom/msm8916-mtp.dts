/dts-v1/;

#include "msm8916-pm8916.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM 8916 MTP";
	compatible = "qcom,msm8916-mtp", "qcom,msm8916";
	chassis-type = "handset";

	aliases {
		serial0 = &blsp_uart2;
		usid0 = &pm8916_0;
	};

	chosen {
		stdout-path = "serial0";
	};
};

&blsp_uart2 {
	status = "okay";
};
