				power-domains = <&pgc_hdmimix>, <&pgc_hdmimix>,
						<&pgc_hdmimix>, <&pgc_hdmimix>,
						<&pgc_hdmimix>, <&pgc_hdmimix>,
						<&pgc_hdmimix>, <&pgc_hdmi_phy>,
						<&pgc_hdmimix>, <&pgc_hdmimix>;
				power-domain-names = "bus", "irqsteer", "lcdif",
