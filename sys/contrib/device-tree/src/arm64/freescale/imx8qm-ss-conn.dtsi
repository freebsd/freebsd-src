&fec1 {
	compatible = "fsl,imx8qm-fec", "fsl,imx6sx-fec";
	iommus = <&smmu 0x12 0x7f80>;
};

&fec2 {
	compatible = "fsl,imx8qm-fec", "fsl,imx6sx-fec";
	iommus = <&smmu 0x12 0x7f80>;
};

&usdhc1 {
	compatible = "fsl,imx8qm-usdhc", "fsl,imx8qxp-usdhc", "fsl,imx7d-usdhc";
	iommus = <&smmu 0x11 0x7f80>;
};

&usdhc2 {
	compatible = "fsl,imx8qm-usdhc", "fsl,imx8qxp-usdhc", "fsl,imx7d-usdhc";
	iommus = <&smmu 0x11 0x7f80>;
};

&usdhc3 {
	compatible = "fsl,imx8qm-usdhc", "fsl,imx8qxp-usdhc", "fsl,imx7d-usdhc";
	iommus = <&smmu 0x11 0x7f80>;
};
