		clocks = <&k3_clks 119 2>;
		clock-names = "fck";
		assigned-clocks = <&k3_clks 119 2>;
		assigned-clock-parents = <&k3_clks 119 3>;
		power-domains = <&k3_pds 119 TI_SCI_PD_EXCLUSIVE>;
		ti,timer-pwm;
