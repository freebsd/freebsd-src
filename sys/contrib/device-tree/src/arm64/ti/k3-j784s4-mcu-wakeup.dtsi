		reg = <0x00 0x40490000 0x00 0x400>;
		interrupts = <GIC_SPI 825 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&k3_clks 125 2>;
		clock-names = "fck";
		assigned-clocks = <&k3_clks 125 2>;
