		clocks = <&k3_clks 36 1>;
		clock-names = "fck";
		assigned-clocks = <&k3_clks 36 1>;
		assigned-clock-parents = <&k3_clks 36 2>;
		power-domains = <&k3_pds 36 TI_SCI_PD_EXCLUSIVE>;
		ti,timer-pwm;
	};

