				compatible = "st,stm32-timers";
				reg = <0x4c00c000 0x400>;
				interrupts = <GIC_SPI 103 IRQ_TYPE_LEVEL_HIGH>;
				interrupt-names = "global";
				clocks = <&rcc TIM17_K>;
				clock-names = "int";
				dmas = <&dmamux1 111 0x400 0x1>,
