/dts-v1/;

#include "qcom-msm8226-microsoft-common.dtsi"

/* This device has no magnetometer */
/delete-node/ &magnetometer;

/ {
	model = "Nokia Lumia 630";
	compatible = "microsoft,moneypenny", "qcom,msm8226";
	chassis-type = "handset";
};

&framebuffer {
	width = <480>;
	height = <854>;
	stride = <(480 * 4)>;
};
