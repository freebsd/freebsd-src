					reg = <0x7>;
				};
			};

			tmp432@4c {
				compatible = "ti,tmp75";
				reg = <0x4c>;
			};
		};

		i2c@7 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x7>;

			i2c-mux@71 {
				compatible = "nxp,pca9546";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x71>;
				i2c-mux-idle-disconnect;

				nvme_m2_0: i2c@0 {
					#address-cells = <1>;
