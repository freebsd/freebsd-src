&cfam0_i2c0 {
	i2cr@20 {
		compatible = "ibm,i2cr-fsi-master";
		reg = <0x20>;
		#address-cells = <2>;
		#size-cells = <0>;

		cfam@0,0 {
			reg = <0 0>;
			#address-cells = <1>;
			#size-cells = <1>;
			chip-id = <0>;

			scom100: scom@1000 {
				compatible = "ibm,i2cr-scom";
				reg = <0x1000 0x400>;
			};

			sbefifo100: sbefifo@2400 {
				compatible = "ibm,p9-sbefifo";
				reg = <0x2400 0x400>;
				#address-cells = <1>;
				#size-cells = <0>;
			};
		};
	};
};

