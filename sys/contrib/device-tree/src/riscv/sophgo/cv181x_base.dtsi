		clock-names = "clk_sys_0", "clk_sys_1",
			"clk_sys_2", "clk_img_d",
			"clk_img_v", "clk_sc_top",
			"clk_sc_d", "clk_sc_v1",
			"clk_sc_v2", "clk_sc_v3";
		clock-freq-vip-sys1 = <300000000>;
