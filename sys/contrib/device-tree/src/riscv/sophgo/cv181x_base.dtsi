		power-ct-gpio = <&porte 1 GPIO_ACTIVE_HIGH>;
		clocks = <&clk CV181X_CLK_DISP_VIP>, <&clk CV181X_CLK_DSI_MAC_VIP>;
		clock-names = "clk_disp", "clk_dsi";
	};

	sys {
		compatible = "cvitek,sys";
