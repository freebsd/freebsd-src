subr_clockcalib.o		standard				\
	dependency	"$S/kern/subr_clockcalib.c"			\
	compile-with	"${CC} -c ${CFLAGS:C/^-O2$/-O3/:N-nostdinc} ${WERROR} -m80387 ${.IMPSRC}" \
	no-implicit-rule						\
	clean		"subr_clockcalib.o"
