dev/agp/agp_amd.c		optional agp
dev/agp/agp_ati.c		optional agp
dev/agp/agp_intel.c		optional agp
dev/agp/agp_nvidia.c		optional agp
dev/agp/agp_sis.c		optional agp
dev/glxiic/glxiic.c		optional glxiic
