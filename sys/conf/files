        before-depend						\
        dependency      "$S/netatm/spans/spans_xdr.x"		\
        compile-with    "rpcgen -h -C $S/netatm/spans/spans_xdr.x > spans_xdr.h" \
        clean           "spans_xdr.h"				\
        no-obj no-implicit-rule
