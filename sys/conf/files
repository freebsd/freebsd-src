        before-depend						\
        dependency      "$S/netatm/spans/spans_xdr.x"		\
        compile-with    "rpcgen -c -C $S/netatm/spans/spans_xdr.x > spans_xdr.c" \
        clean           "spans_xdr.c"				\
        no-obj no-implicit-rule local
