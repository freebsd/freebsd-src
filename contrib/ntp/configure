 no*) ;;
 yes.) ;;
 *) case "`${MAKE-make} -v -f /dev/null 2>/dev/null | sed -e 's/GNU Make version \(1-9.]*\).*/\1/' -e q`" in
     '')
	as_fn_error "building outside of the main directory requires GNU make" "$LINENO" 5
	;;
     *) ;;
    esac
    ;;
esac





