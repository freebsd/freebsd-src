 static void
Qfpbits(U *u, FPBits *b)
{
	ULong *bits;
	int ex, i;
	static FPI fpi0 = { 113, 1-16383-113+1, 32766 - 16383 - 113 + 1, 1, 0 };

	b->fpi = &fpi0;
	b->gdtoa = gdtoa;
	b->sign = u->ui[_0] & 0x80000000L;
	bits = b->bits;
	bits[3] = u->ui[_0] & 0xffff;
	bits[2] = u->ui[_1];
	bits[1] = u->ui[_2];
	bits[0] = u->ui[_3];
	if ( (ex = (u->ui[_0] & 0x7fff0000L) >> 16) !=0) {
		if (ex == 0x7fff) {
			/* Infinity or NaN */
			i = bits[0] | bits[1] | bits[2] | bits[3]
				? STRTOG_NaN : STRTOG_Infinite;
			}
		else {
			i = STRTOG_Normal;
			bits[3] |= 0x10000;
			}
		}
	else if (bits[0] | bits[1] | bits[2] | bits[3]) {
		i = STRTOG_Denormal;
		ex = 1;
		}
	else
		i = STRTOG_Zero;
	b->kind = i;
	b->ex = ex - (0x3fff + 112);
	}

#undef _0
#undef _1
