.    nr doc-arg-ptr +1
.    if (\n[doc-arg-limit] >= \n[doc-arg-ptr]) \{\
.      nr doc-curr-font \n[.f]
.      nr doc-curr-size \n[.ps]
.      nop \*[doc-Fa-font]\c
.      doc-print-recursive
.
.      if \n[doc-in-synopsis-section] \
