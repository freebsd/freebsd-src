# This rule was used for the check-cdtest target.
#.cc.o:
#	$(CXX) -c -I$(srcdir) $(CXXFLAGS) $(CFLAGS) $<

ALL_CFLAGS = -D_GNU_SOURCE $(INCLUDES) @HDEFINES@ $(CFLAGS)
.c.o:
	$(CC) -c $(ALL_CFLAGS) $<

