	@$(MAKE) $(FLAGS_TO_PASS) DO=maintainer-clean "DODIRS=$(SUBDIRS)" subdir_do
	rm -f c-exp.tab.c f-exp.tab.c m2-exp.tab.c
	rm -f TAGS $(INFOFILES)
	rm -f nm.h tm.h xm.h config.status
	rm -f y.output yacc.acts yacc.tmp
	rm -f config.log config.cache
	rm -f Makefile

