/build_sq {
    pop
    size 2 div /side exch def
    currentpoint
    newpath
    moveto
    0 side rlineto
    side 0 rlineto
    0 side neg rlineto
    closepath
    currentfont dup /FontInfo dup 3 1 roll known {
        get dup /Weight dup 3 1 roll known {
            get dup dup (Ultra) eq exch (Bold) eq or exch (Heavy) eq or
	    	{fill} {stroke} ifelse
        } {pop pop stroke} ifelse
    } {pop pop stroke} ifelse
} def
%%EndResource
