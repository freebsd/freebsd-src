\eke	goto-end
\eeG	goto-end-buffered
\&=	status
^G	status
:f	status
/	forw-search
?	back-search
\ee/	forw-search *
\ee?	back-search *
n	repeat-search
\een	repeat-search-all
N	reverse-search
\eeN	reverse-search-all
&	filter
m	set-mark
M	set-mark-bottom
\eem	clear-mark
'	goto-mark
^X^X	goto-mark
E	examine
