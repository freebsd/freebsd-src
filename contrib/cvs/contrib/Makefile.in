	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(contribdir)
	@list='$(contrib_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " $(INSTALL_DATA) $$d$$p $(DESTDIR)$(contribdir)/$$f"; \
	  $(INSTALL_DATA) $$d$$p $(DESTDIR)$(contribdir)/$$f; \
	done

