    contrib/cvs_acls ) chmod +x contrib/cvs_acls ;;
    contrib/log ) chmod +x contrib/log ;;
    contrib/log_accum ) chmod +x contrib/log_accum ;;
    contrib/mfpipe ) chmod +x contrib/mfpipe ;;
