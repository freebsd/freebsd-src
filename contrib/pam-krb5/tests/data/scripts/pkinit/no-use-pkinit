[options]
    auth = no_ccache use_pkinit

[run]
    authenticate = PAM_AUTHINFO_UNAVAIL

[output]
    ERR use_pkinit requested but PKINIT not available or cannot be enforced
    NOTICE authentication failure; logname=%u uid=%i euid=%i tty= ruser= rhost=
