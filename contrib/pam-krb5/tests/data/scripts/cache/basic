[options]
    auth    = force_first_pass ignore_k5login
    account = ignore_k5login

[run]
    authenticate  = PAM_SUCCESS
    acct_mgmt     = PAM_SUCCESS
    open_session  = PAM_SUCCESS
    close_session = PAM_SUCCESS

[output]
    INFO user %u authenticated as %0
