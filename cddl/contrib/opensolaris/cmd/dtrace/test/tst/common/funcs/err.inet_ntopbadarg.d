#pragma ident	"%Z%%M%	%I%	%E% SMI"

#pragma D option quiet

ipaddr_t *ip4a;

BEGIN
{
	this->buf4a = alloca(sizeof (ipaddr_t));
	ip4a = this->buf4a;
	*ip4a = htonl(0xc0a80117);

	printf("%s\n", inet_ntop(-1, ip4a));

	exit(0);
}

ERROR
{
	exit(1);
}
