	echo "basename(\"/foo/bar/baz/.\") is \".\"; expected \"`basename "/foo/bar/baz/."`"\"
fi

if [ `dirname "/foo/bar/baz/."` != "/foo/bar/baz" ]; then
	echo "dirname(\"/foo/bar/baz/.\") is \"/foo/bar/baz\"; expected \"`dirname "/foo/bar/baz/."`"\"
fi

if [ `basename "/foo/bar/baz/./"` != "." ]; then
	echo "basename(\"/foo/bar/baz/./\") is \".\"; expected \"`basename "/foo/bar/baz/./"`"\"
fi

if [ `dirname "/foo/bar/baz/./"` != "/foo/bar/baz" ]; then
	echo "dirname(\"/foo/bar/baz/./\") is \"/foo/bar/baz\"; expected \"`dirname "/foo/bar/baz/./"`"\"
fi

if [ `basename "/foo/bar/baz/.//"` != "." ]; then
	echo "basename(\"/foo/bar/baz/.//\") is \".\"; expected \"`basename "/foo/bar/baz/.//"`"\"
fi

if [ `dirname "/foo/bar/baz/.//"` != "/foo/bar/baz" ]; then
