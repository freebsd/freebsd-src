#!/usr/bin/env ksh

if [ `basename "/foo/bar/baz"` != "baz" ]; then
	echo "basename(\"/foo/bar/baz\") is \"baz\"; expected \"`basename "/foo/bar/baz"`"\"
fi

if [ `dirname "/foo/bar/baz"` != "/foo/bar" ]; then
	echo "dirname(\"/foo/bar/baz\") is \"/foo/bar\"; expected \"`dirname "/foo/bar/baz"`"\"
fi

if [ `basename "/foo/bar///baz/"` != "baz" ]; then
	echo "basename(\"/foo/bar///baz/\") is \"baz\"; expected \"`basename "/foo/bar///baz/"`"\"
fi

if [ `dirname "/foo/bar///baz/"` != "/foo/bar" ]; then
	echo "dirname(\"/foo/bar///baz/\") is \"/foo/bar\"; expected \"`dirname "/foo/bar///baz/"`"\"
fi

if [ `basename "/foo/bar/baz/"` != "baz" ]; then
	echo "basename(\"/foo/bar/baz/\") is \"baz\"; expected \"`basename "/foo/bar/baz/"`"\"
