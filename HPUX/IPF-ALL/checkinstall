CONTRIBDIR=/usr/contrib/ipf
########
# Source control script environment
########

UTILS="/usr/lbin/sw/control_utils"
if [[ ! -f $UTILS ]] ; then
    echo "ERROR: Cannot find $UTILS"
    exit 1
fi
. $UTILS
exitval=$SUCCESS

########
# Make sure that the header files exist
########

exit $exitval
