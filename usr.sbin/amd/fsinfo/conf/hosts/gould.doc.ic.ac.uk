		"
	}
}
	
// spool stuff, including the news
fs /dev/dk4f {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 5
	mount /var/spool {
		exportfs "\
			oriona \
			rpcsfg \
		"
	}
}
	
fs /dev/dk3h {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 2
	mount /var/spool/News {}
}
	
// this is the public ftp area
fs /dev/dk3f {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 4
	mount /usr/reserve {
		PUBLIC {
			volname /vol/public
			exportfs "\
				oriona \
				ssun1.doc.ic.ac.uk \
				ssun2.doc.ic.ac.uk \
				obsidian \
				gummo \
				tsunfs.doc.ic.ac.uk \
				flamingo.doc.ic.ac.uk \
			"
		}
	}
}
	
// sources - local and public
fs /dev/dk7c {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 2
	dumpset = csg_nightly
	mount /usr/src {
		volname /vol/src/gould
		exportfs "\
			flamingo.doc.ic.ac.uk \
			pelican.doc.ic.ac.uk \
			oriona \
			ssun1.doc.ic.ac.uk \
			svax.doc.ic.ac.uk \
			rvax.doc.ic.ac.uk \
			obsidian \
			tsunfs \
		"
	}
}
	
// sources - utx
fs /dev/dk4e {
