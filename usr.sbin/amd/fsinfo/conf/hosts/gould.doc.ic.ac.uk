// booting diskless suns
fs /dev/dk5e {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 3
	dumpset = csg_nightly
	mount /export {
#ifndef ok
		volname /vol/export
#endif
		exportfs "\
			-rootid=0 \
			whoops \
			whoops.doc.ic.ac.uk \
		"
		misc {
			volname /vol/export/misc
		}
	}
}
	
