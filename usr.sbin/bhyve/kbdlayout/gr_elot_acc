'"',0x1e;	"  (0x22)
'#',0x5d;	#  (0x23)
''',0x0e;	'  (0x27)
'*',0xb7;	*  (0x2a)
'/',0x52;	/  (0x2f)
'@',0x52;	@  (0x40)
'\',0x61;	\  (0x5c)
'|',0x61;	|  (0x7c)
'~',0x5d;	~  (0x7e)

