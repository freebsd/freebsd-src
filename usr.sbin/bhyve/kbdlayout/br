# bhyve ps2kbd keyboard layout file
#   (br : Brazilian (accent keys))
#
# Created by: Koine Yuusuke(koinec) <koinec@users.osdn.me>
#

'"',0x0e;	"  (0x22)
''',0x0e;	'  (0x27)
':',0x4a;	:  (0x3a)
';',0x4a;	;  (0x3b)
'?',0x51;	?  (0x3f)
'[',0x5b;	[  (0x5b)
'\',0x61;	\  (0x5c)
']',0x5d;	]  (0x5d)
'{',0x5b;	{  (0x7b)
'|',0x61;	|  (0x7c)
'}',0x5d;	}  (0x7d)

