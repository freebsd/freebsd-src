rw.o: ../../include/version.h

all-local: $(inc_DATA)
	@for i in $(inc_DATA); do \
		if cmp -s  $$i $(idir)/$$i 2> /dev/null ; then :; else\
			echo " $(CP) $$i $(idir)/$$i"; \
			$(CP) $$i $(idir)/$$i; \
		fi ; \
	done

