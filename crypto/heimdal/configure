        64) abi='-mabi=64';  abilibdirext='64'   ;;
	no) abi=''; abilibdirext='';;
         *) { { echo "$as_me:$LINENO: error: \"Invalid ABI specified\"" >&5
echo "$as_me: error: \"Invalid ABI specified\"" >&2;}
   { (exit 1); exit 1; }; } ;;
