# $FreeBSD$

#	dump.h			header file
#	itime.c			reads /etc/dumpdates
#	main.c			driver
#	optr.c			operator interface
#	dumprmt.c		handles remote tape via rmt(8)
#	tape.c			handles the mag tape and opening/closing
#	traverse.c		traverses the file system
#	unctime.c		undo ctime
#
#	DEBUG			use local directory to find ddate and dumpdates
#	TDEBUG			trace out the process forking

PROG = dump

SRCS = \
	cache.c \
	dumprmt.c \
	itime.c \
	main.c \
	optr.c \
	tape.c \
	traverse.c \
	unctime.c

CFLAGS += -DRDUMP

DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/machine include/protocols lib/csu/MACHINE_ARCH lib/libc stage sys/MACHINE_ARCH/include sys/bsm sys/netinet sys/netinet6 sys/sys sys/ufs/ffs sys/ufs/ufs
.include <bsd.prog.mk>
