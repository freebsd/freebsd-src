#, c-format
msgid "  Streams:            %s\n"
msgstr "  Stream:               %s\n"

#: ../src/xz/list.c:654
#, c-format
msgid "  Blocks:             %s\n"
msgstr "  Blocchi:              %s\n"

#: ../src/xz/list.c:656
#, c-format
