#include <sys/types.h>
#include <sys/socket.h>
#include <netinet/in.h>
#include <arpa/inet.h>
int
main ()
{
char src[4], dst[128];
inet_ntop(AF_INET, src, dst, sizeof(dst));
  ;
  return 0;
